    <DataSet Name="Permit_Types">
      <Query>
        <DataSourceName>DataSource_BuildingPermits_Data</DataSourceName>
        <CommandText>select [PermitTypeSK], [AlternatePermitTypeId],[PermitTypeDefinition]
